{
  "global_tags": {
    "owning_team": "OpenSource",
    "Environment": "development",
    "Department": "Product",
    "Product": "Virtuoso"
  },
  "resources": [
    {
      "id": "host",
      "display_name": "AwsLinux2InfraMongoDBInstallHost",
      "provider": "aws",
      "type": "ec2",
      "size": "t3.small",
      "ami_name": "amazonlinux-2-base*",
      "user_name": "ec2-user"
    }
  ],
  "services": [
    {
      "id": "docker",
      "destinations": [
        "host"
      ],
      "local_source_path":"/mnt/deployer/open-install-library",
      "deploy_script_path": "test/deploy/linux/docker/install/roles",
      "port": 9999
    },
    {
      "id": "mongo",
      "destinations": [
        "host"
      ],
      "local_source_path":"/mnt/deployer/open-install-library",
      "deploy_script_path": "test/deploy/linux/mongodb/install/docker/roles",
      "port": 27017
    }
  ]
}