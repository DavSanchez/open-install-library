{
  "global_tags": {
    "owning_team": "OpenSource",
    "Environment": "development",
    "Department": "Product",
    "Product": "Virtuoso"
  },
  "resources": [
    {
      "id": "e2e-hack-box",
      "provider": "aws",
      "type": "ec2",
      "size": "t2.micro",
      "ami_name": "amazonlinux-2-base*",
