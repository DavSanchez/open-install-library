{
    "global_tags": {
      "owning_team": "OpenSource",
      "Environment": "development",
      "Department": "Product",
      "Product": "Virtuoso"
    },
  
    "resources": [{
      "id": "netlinux2",
      "display_name": "NetLinux2Host",
      "provider": "aws",
      "type": "ec2",
      "size": "t3a.micro",
      "ami_name": "amzn2-x86_64-MATEDE_DOTNET-*"
    }],

    "services": [{
      "id": "webapp1",
      "display_name": "Web Portal (systemctl)",
      "destinations": ["netlinux2"],
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/.net/deploy-webapplication/roles",
      "port": 9091
    },{
      "id": "webapp2",
      "display_name": "Routing Service (systemctl)",
      "destinations": ["netlinux2"],
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/.net/deploy-webapplication/roles",
      "port": 9092
    },{
      "id": "webapp3",
      "display_name": "Core Login Service (systemctl)",
      "destinations": ["netlinux2"],
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/.net/deploy-webapplication/roles",
      "port": 9093
    }]
}