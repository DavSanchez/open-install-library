{
    "global_tags": {
      "owning_team": "OpenSource",
      "Environment": "development",
      "Department": "Product",
      "Product": "Virtuoso"
    },
  
    "resources": [{
      "id": "host1",
      "display_name": "SuseInfraHashiCorpConsulInstallHost",
      "provider": "aws",
      "type": "ec2",
      "size": "t3.small",
      "ami_name": "suse-sles-12-sp4-*"
    }],
  
    "services": [{
      "id": "consul1",
      "destinations": ["host1"],
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/hashi-corp-consul/install/suse/roles",
      "port": 9999
    }],
  
    "instrumentations": {
      "resources": [{
          "id": "nr_infra_hashi-corp-consul",
          "resource_ids": ["host1"],
          "provider": "newrelic",
          "source_repository": "https://github.com/newrelic/open-install-library.git",
          "deploy_script_path": "test/deploy/linux/newrelic-cli/install-recipe/roles",
          "params": {
            "recipe_content_url": [
              "https://raw.githubusercontent.com/newrelic/open-install-library/main/recipes/newrelic/infra-agent/suse.yml",
              "https://raw.githubusercontent.com/newrelic/open-install-library/main/recipes/newrelic/hashi-corp-consul/suse.yml"
            ]
          }
        },
        {
          "id": "nr_infra_is_having_data",
          "resource_ids": ["host1"],
          "provider": "newrelic",
          "source_repository": "https://github.com/newrelic/open-install-library",
          "deploy_script_path": "test/deploy/assertions/recipe-is-valid/roles",
          "params": {
            "recipe_validation_nrql_url": "https://raw.githubusercontent.com/newrelic/open-install-library/main/recipes/newrelic/hashi-corp-consul/suse.yml"
          }
        }
      ]
    }
  }
